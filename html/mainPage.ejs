<html>
  <head>
    <meta charset="UTF-8" />
    <link rel="stylesheet" href="css/mainPage.css" />
    <title>main</title>
  </head>
  <body>
    <div id="root">
      <header>
        <div>안녕하세요 <%= admin %> 님</div>
        <% if(admin === '0') { %>
        <form action="/myPage" method="post">
          <input type="submit" value="마이페이지" />
        </form>
        <% } else if(admin === '1') {%>
        <form action="/admin" method="post">
          <input type="submit" value="관리자페이지" />
        </form>
        <% }%>
      </header>
      <main>
        <div>
          <div id="list">
            <% for(let i=0; i<10; i++) {%>
            <div>
              <div><%= list[i].header %></div>
              <div><%= list[i].user_id %></div>
              <div><%= list[i].update_date.toLocaleString() %></div>
            </div>
            <% } %>
          </div>
          <div></div>
        </div>
      </main>
    </div>
  </body>
</html>
